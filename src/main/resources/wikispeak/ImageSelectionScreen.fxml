<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>

<BorderPane fx:id="rootBorderPane" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wikispeak.ImageSelectionController">
   <center>
      <AnchorPane fx:id="myAnchor1" prefHeight="400.0" prefWidth="598.0" BorderPane.alignment="CENTER">
         <children>
            <ScrollPane layoutX="14.0" layoutY="14.0" prefHeight="282.0" prefWidth="361.0">
               <content>
                  <TilePane fx:id="tilePane" prefHeight="278.0" prefWidth="343.0" />
               </content>
            </ScrollPane>
            <ScrollPane layoutX="388.0" layoutY="14.0" prefHeight="282.0" prefWidth="198.0">
               <content>
                  <TilePane fx:id="selectedImages" prefHeight="279.0" prefWidth="141.0" />
               </content>
            </ScrollPane>
            <Button fx:id="createButton1" layoutX="388.0" layoutY="296.0" mnemonicParsing="false" onAction="#handleCreate" prefHeight="89.0" prefWidth="198.0" text="Create" />
            <Button layoutX="14.0" layoutY="350.0" mnemonicParsing="false" onAction="#habdleBackButton" prefHeight="37.0" prefWidth="114.0" text="Back" />
            <Button layoutX="138.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleMainMenuButton" prefHeight="37.0" prefWidth="114.0" text="Main Menu" />
            <Button fx:id="getImageButton" layoutX="14.0" layoutY="296.0" mnemonicParsing="false" onAction="#handleGetImageButton" prefHeight="37.0" prefWidth="361.0" text="Get Images From Flickr" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
