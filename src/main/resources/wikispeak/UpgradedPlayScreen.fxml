            <Button fx:id="playPauseButton" layoutX="174.0" layoutY="362.0" mnemonicParsing="false" onAction="#handlePlayPauseButton" text="Play/Pause" textAlignment="CENTER" />
            <Button layoutX="14.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleExitButton" prefHeight="18.0" prefWidth="156.0" text="Exit" />
