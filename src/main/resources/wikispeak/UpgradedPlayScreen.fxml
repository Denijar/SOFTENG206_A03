            <Button layoutX="14.0" layoutY="331.0" mnemonicParsing="false" onAction="#handleDeleteButton" prefHeight="26.0" prefWidth="157.0" text="Delete" />
            <Label fx:id="listIsEmpty" contentDisplay="CENTER" layoutX="24.0" layoutY="25.0" prefHeight="293.0" prefWidth="136.0" textAlignment="CENTER" />
            <Label layoutX="355.0" layoutY="367.0" text="Volume" />
         </children>
      </AnchorPane>
   </bottom>
